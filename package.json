{
  "name": "gaiafest-static",
  "version": "1.0.0",
  "description": "",
  "main": "sass/mystyles.scss",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss css/mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "build": "showdown makehtml -i content.md -o content.html && cat page-start.html content.html page-end.html >index.html",
    "showdown": "showdown makehtml -i content.md -o content.html",
    "start": "npm run css-watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bulma": "^0.9.2",
    "node-sass": "^5.0.0",
    "showdown": "^1.9.1"
  }
}
